# Chrome Extension Environment Variables
# Copy this file to .env and fill in your values

# Comma-separated list of URL patterns where the extension can run
# Examples:
#   Single domain: https://example.com/*
#   Multiple: https://example.com/*,https://api.example.com/*
#   Subdomains: https://*.example.com/*
#   Development: https://localhost:3000/*
SUPPORTED_DOMAINS=https://localhost:2222/*

# Extension display name (shown in Chrome)
EXTENSION_NAME=Chrome Extension

# Optional: URL to release notes / changelog
RELEASE_SPACE_URL=

# Optional: URL for user feedback / bug reports
FEEDBACK_SPACE_URL=
